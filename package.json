{
  "name": "blind_beat",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "clean": "rm -fR ./dist",
    "rebuild": "rm -fR ./dist; tsc && npm dist/index.js",
    "db:check": "prisma studio",
    "db:start": "docker-compose up -d mongodb",
    "db:stop": "docker-compose down -v",
    "db:update": "prisma generate",
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "start": "node dist/index.js",
    "commands:update": "npm ./src/deploy-commands.ts",
    "lint": "npx eslint --config config/.eslintrc.yml --ignore-path config/.eslintignore --ext .ts .",
    "lint:fix": "npx eslint --config config/.eslintrc.yml --ignore-path config/.eslintignore --ext .ts . --fix"
  },
  "private": true,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.0.1",
    "@discordjs/voice": "^0.16.1",
    "@prisma/client": "^5.4.2",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.13",
    "node-cron": "^3.0.2",
    "nodemon": "^3.1.0",
    "winston": "^3.11.0",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "bun-types": "^1.0.1",
    "discord-api-types": "^0.37.56",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
